    <rng:define name="BaseWithoutId">

        <a:documentation>
            Base element without ID specified *yet*, e.g. for an element with a particular requirement on its id which does not comply with NmlId (e.g. Segment needs nonNegativeInteger).
        </a:documentation>

        <rng:optional><rng:attribute name="neuroLexId"><rng:ref name="NeuroLexId"/></rng:attribute></rng:optional>

    </rng:define>
    
    <rng:define name="Base">

        <a:documentation>
            Anything which can have a unique (within its parent) id of the form NmlId (spaceless combination of letters, numbers and underscore).
        </a:documentation>
        
        
            <rng:ref name="BaseWithoutId"/>

                <rng:attribute name="id"><rng:ref name="NmlId"/></rng:attribute>
                
            
        

    </rng:define>
    


