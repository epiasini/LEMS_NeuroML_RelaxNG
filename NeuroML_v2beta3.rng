    <rng:define name="GateHHRatesInf">
        
            <rng:ref name="Base"/>
                <rng:interleave><rng:optional><rng:element name="notes"><rng:ref name="Notes"/></rng:element></rng:optional><rng:optional><rng:element name="q10Settings"><rng:ref name="Q10Settings"/></rng:element></rng:optional><rng:element name="forwardRate"><rng:ref name="HHRate"/></rng:element><rng:element name="reverseRate"><rng:ref name="HHRate"/></rng:element><rng:element name="steadyState"><rng:ref name="HHVariable"/></rng:element></rng:interleave>
                <rng:optional><rng:attribute xmlns:ns_1="http://relaxng.org/ns/compatibility/annotations/1.0" name="instances" ns_1:defaultValue="1"><rng:data type="integer"/></rng:attribute></rng:optional>
